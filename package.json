{
  "name": "my-jsql",
  "version": "1.5.1",
  "description": "Easy to use SQL query module for passing JavaScript objects instead of manually writing SQL",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint src -c .eslintrc.js; eslint test -c .eslintrc.js",
    "fix": "eslint src -c .eslintrc.js --fix; eslint test -c .eslintrc.js --fix",
    "test": "mocha test",
    "build": "npm run lint; babel src --presets babel-preset-es2015 --out-dir dist --minified --no-comments; npm test",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z3nz/my-jsql.git"
  },
  "keywords": [
    "mysql",
    "my",
    "sql",
    "json",
    "jsql",
    "jssql",
    "javascript",
    "object"
  ],
  "author": "Adam Lynch <adamlynchmob@gmail.com> (https://github.com/z3nz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/z3nz/my-jsql/issues"
  },
  "homepage": "https://github.com/z3nz/my-jsql#readme",
  "dependencies": {
    "mysql": "^2.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "editorconfig": "^0.13.2",
    "eslint": "^4.2.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jasmine": "^2.8.4",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.4.2",
    "unit.js": "^2.0.0"
  }
}
